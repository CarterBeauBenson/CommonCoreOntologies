###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002000
cco:CCO_0002000 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf obo:RO_0000056 ;
                owl:inverseOf cco:CCO_0002017 ;
                rdfs:domain obo:BFO_0000002 ;
                rdfs:range obo:BFO_0000015 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "c is_affected_by p iff p is an instance of a Process and c is an instance of a Continuant, such that p influences c in some manner, most often by producing a change in c."@en ;
                rdfs:label "is affected by"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002001
cco:CCO_0002001 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf obo:RO_0000056 ;
                owl:inverseOf cco:CCO_0002015 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "If p is a process and c is a continuant, then c is object of p if and only if the c is part of the projected state that the agent intends to achieve by performing p."@en ;
                rdfs:label "is object of"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002002
cco:CCO_0002002 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf obo:RO_0000057 ;
                owl:inverseOf cco:CCO_0002006 ;
                rdfs:domain obo:BFO_0000015 ;
                rdfs:range obo:BFO_0000040 ;
                cco:CCO_0002036 "https://en.wikipedia.org/w/index.php?title=Accomplice&oldid=1002047204"^^xsd:anyURI ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Processual Entity p1 has_accomplice some agent a1 iff a1 assists in the commission of p1, is located at the location of p1, but is not agent_in p1."@en ;
                rdfs:label "has accomplice"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002003
cco:CCO_0002003 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf cco:CCO_0002028 ;
                rdfs:domain obo:BFO_0000030 ;
                rdfs:range obo:BFO_0000030 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "An object o is made of an object m when m is the material that o consists of and that material does not undergo a change of kind during the creation of o"@en ;
                rdfs:label "is made of"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002004
cco:CCO_0002004 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0002022 ;
                rdfs:domain obo:BFO_0000004 ;
                rdfs:range obo:BFO_0000004 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002059 "More informally, c1 is a predecessor of c2 iff c1 has been followed or replaced by c2."@en ;
                cco:CCO_0002060 "A continuant c1 is a predecessor of some continuant c2 iff there is some process p1 and c1 is an input to p1 and c2 is an output of p1."@en ;
                rdfs:label "is predecessor of"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002005
cco:CCO_0002005 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0002031 ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( obo:BFO_0000020
                                            obo:BFO_0000031
                                          )
                            ] ;
                rdfs:range obo:BFO_0000027 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "x inheres_in_aggregate y iff x is an instance of Specifically Dependent Continuant and y is an instance of Object Aggregate and z is an instance of Object, such that z bearer_of x, and all other members of y are bearers of a unique instance of the same type as x."@en ;
                rdfs:label "inheres in aggregate"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002006
cco:CCO_0002006 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf obo:RO_0000056 ;
                rdfs:domain obo:BFO_0000040 ;
                rdfs:range obo:BFO_0000015 ;
                cco:CCO_0002036 "https://en.wikipedia.org/w/index.php?title=Accomplice&oldid=1002047204"^^xsd:anyURI ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "An agent a1 is accomplice_in some Processual Entity p1 iff a1 assists in the commission of p1, is located at the location of p1, but is not agent_in p1."@en ;
                rdfs:label "accomplice in"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002007
cco:CCO_0002007 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0002029 ;
                rdfs:domain obo:BFO_0000015 ;
                rdfs:range obo:BFO_0000008 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "x occurs_on y iff x is an instance of a process or process boundary and y is an instance of a temporal region, such that the duration of x temporally projects on y."@en ;
                rdfs:label "occurs on"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002008
cco:CCO_0002008 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0002005 ;
                rdfs:domain obo:BFO_0000019 ;
                rdfs:range obo:BFO_0000027 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "x quality_of_aggregate y iff y is an instance of Object Aggregate and x is an instance of Quality, such that x disposition_of_aggregate y."@en ;
                rdfs:label "quality of aggregate"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002009
cco:CCO_0002009 rdf:type owl:ObjectProperty ;
                rdfs:domain obo:BFO_0000027 ;
                rdfs:range obo:BFO_0000040 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "An instance of an Object Aggregate 'has member of located in' an instance of some material entity if and only if every member of that Aggregate is located in the same instance of that material entity."@en ;
                rdfs:label "has member of located in"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002010
cco:CCO_0002010 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf obo:RO_0000057 ;
                owl:inverseOf cco:CCO_0002027 ;
                rdfs:domain obo:BFO_0000015 ;
                rdfs:range obo:BFO_0000002 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "y has_input x iff x is an instance of Continuant and y is an instance of Process, such that the presence of x at the beginning of y is a necessary condition for the start of y."@en ;
                rdfs:label "has input"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002011
cco:CCO_0002011 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0002005 ;
                rdfs:domain obo:BFO_0000023 ;
                rdfs:range obo:BFO_0000027 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "x role_of_aggregate y iff y is an instance of Object Aggregate and x is an instance of Role, such that x disposition_of_aggregate y."@en ;
                rdfs:label "role of aggregate"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002012
cco:CCO_0002012 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0002031 ;
                rdfs:domain obo:BFO_0000027 ;
                rdfs:range obo:BFO_0000019 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "x aggregate_has_quality y iff x is an instance of Object Aggregate and y is an instance of Quality, such that x aggregate_bearer_of y."@en ;
                rdfs:label "aggregate has quality"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002013
cco:CCO_0002013 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0002030 ;
                rdfs:domain obo:BFO_0000003 ;
                rdfs:range obo:BFO_0000003 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "x caused_by y iff x and y are instances of occurrents, and x is a consequence of y."@en ;
                rdfs:label "caused by"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0002014
cco:CCO_0002014 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0002030 ;
                owl:inverseOf cco:CCO_0002024 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                cco:CCO_0002059 "A process x starts another process y when x causes y while x is still occurring."@en ;

#################################################################
#    Object Properties
#################################################################

###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009033
cco:CCO_0009033 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0009048 ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000148 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "For Temporal Interval t1 and Temporal Instant t2, t1 has inside instant t2 if and only if there exists Temporal Instants t3 and t4 that are part of t1 and non-identical with t2, such that t3 is before t2 and t4 is after t2."@en ;
                rdfs:label "has inside instant"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009034
cco:CCO_0009034 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0009042 ;
                owl:inverseOf cco:CCO_0009040 ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A TemporalInterval INT2 is after some TemporalInterval INT1 iff there exists TemporalInstants inst2, inst1 such that inst2 is the starting instant of INT2 and inst1 is the ending instant of INT1 and inst2 is after inst1."@en ;
                rdfs:label "interval is after"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009035
cco:CCO_0009035 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0009042 ;
                owl:inverseOf cco:CCO_0009051 ;
                rdf:type owl:IrreflexiveProperty ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Temporal Interval INT2 is met by some Temporal Interval INT1 iff there exists some Temporal Instant inst1 such that inst1 is the starting instant of INT2 and inst1 is the ending instant of INT1."@en ;
                rdfs:label "interval met by"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009036
cco:CCO_0009036 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0009041 ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain obo:BFO_0000148 ;
                rdfs:range obo:BFO_0000148 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002059 "'instant is after' is a primitive relationship. Informally, a temporal instant t2 is after some temporal instant t1 if and only if t1 precedes t2."@en ;
                cco:CCO_0002060 "A temporal instant t2 (a instance of a zero-dimensional temporal region) is after another temporal instant t1 iff there exists some instance of a temporal interval ti1 such that t1 is the starting instant of ti1 and t2 is the ending instant of ti1."@en ;
                rdfs:label "instant is after"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009037
cco:CCO_0009037 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0009039 ;
                owl:inverseOf cco:CCO_0009045 ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Temporal Interval INT1 is during some Temporal Interval INT2 iff there exist Temporal Instants inst1, inst2, inst3, and inst4 such that inst1 is the starting instant of INT1, inst2 is the ending instant of INT1, inst3 is the starting instant of INT2, inst4 is the ending instant of INT2, inst3 is before inst1, and inst2 is before inst4."@en ;
                rdfs:label "interval during"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009038
cco:CCO_0009038 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0009052 ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000148 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "For Temporal Interval t1 and Temporal Instant t2, t1 has ending instant t2 if and only if no Temporal Instant t3 that is part of t1 is after t2."@en ;
                rdfs:label "has ending instant"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009039
cco:CCO_0009039 rdf:type owl:ObjectProperty ;
                owl:inverseOf cco:CCO_0009044 ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                owl:propertyDisjointWith cco:CCO_0009042 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Temporal Interval INT1 is contained by some Temporal Interval INT2 iff there exist Temporal Instants inst1, inst2, inst3, and inst4 such that inst1 is the starting instant of INT1, inst2 is the ending instant of INT1, inst3 is the starting instant of INT2, inst4 is the ending instant of INT2, inst3 is before or identical to inst1, inst2 is before or identical to inst4, and it is not the case that both inst3 is identical to inst1 and inst2 is identical to inst4."@en ;
                rdfs:label "interval contained by"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009040
cco:CCO_0009040 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0009042 ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A TemporalInterval INT1 is before some TemporalInterval INT2 iff there exists TemporalInstants inst1, inst2 such that inst1 is the ending instant of INT1 and inst2 is the starting instant of INT2 and inst1 is before inst2."@en ;
                rdfs:label "interval is before"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009041
cco:CCO_0009041 rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain obo:BFO_0000148 ;
                rdfs:range obo:BFO_0000148 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002059 "'instant is before' is a primitive relationship. Informally, a temporal instant t1 is before some temporal instant t2 if and only if t1 precedes t2."@en ;
                cco:CCO_0002060 "A temporal instant t1 (a instance of a zero-dimensional temporal region) is before another temporal instant t2 iff there exists some instance of a temporal interval ti1 such that t1 is the starting instant of ti1 and t2 is the ending instant of ti1."@en ;
                rdfs:label "instant is before"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009042
cco:CCO_0009042 rdf:type owl:ObjectProperty ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                owl:propertyDisjointWith cco:CCO_0009044 ,
                                         cco:CCO_0009047 ,
                                         cco:CCO_0009054 ,
                                         cco:CCO_0009055 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Temporal Interval INT1 is disjoint with a Temporal Interval INT2 iff INT1 is before or meets INT2 OR INT2 is before or meets INT1. In other words, INT1 and INT2 are disjoint iff INT1 and INT2 do not overlap, contain, or equal one another."@en ;
                rdfs:label "interval disjoint"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009043
cco:CCO_0009043 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0009044 ;
                owl:inverseOf cco:CCO_0009046 ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Temporal Interval INT2 is started by some Temporal Interval INT1 iff there exist Temporal Instants inst1, inst2, and inst3 such that inst1 is the starting instant of both INT1 and INT2, inst2 is the ending instant of INT1, inst3 is the ending instant of INT2 and inst2 is before inst3."@en ;
                rdfs:label "interval started by"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009044
cco:CCO_0009044 rdf:type owl:ObjectProperty ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Temporal Interval INT2 contains some Temporal Interval INT1 iff there exist Temporal Instants inst1, inst2, inst3, and inst4 such that inst1 is the starting instant of INT1, inst2 is the ending instant of INT1, inst3 is the starting instant of INT2, inst4 is the ending instant of INT2, inst3 is before or identical to inst1, and inst2 is before or identical to inst4, but it is not the case that both inst3 is identical to inst1 and inst2 is identical to inst4."@en ;
                rdfs:label "interval contains"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009045
cco:CCO_0009045 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cco:CCO_0009044 ;
                rdfs:domain obo:BFO_0000038 ;
                rdfs:range obo:BFO_0000038 ;

###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009053
cco:CCO_0009053 rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain obo:BFO_0000038 ;

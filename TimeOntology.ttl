                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A One-Dimensional Temporal Region that is equal to sixty consecutive Minutes."@en ;
                rdfs:label "Hour"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009005
cco:CCO_0009005 rdf:type owl:Class ;
                rdfs:subClassOf obo:BFO_0000038 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A One-Dimensional Temporal Region that consists of the temporal regions between when people typically retire to sleep (approximately 9:00pm) and when the sun dawns (approximately 6:00am)."@en ;
                rdfs:label "Night"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009006
cco:CCO_0009006 rdf:type owl:Class ;
                rdfs:subClassOf obo:BFO_0000038 ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty cco:CCO_0009037 ;
                                  owl:someValuesFrom cco:CCO_0009023
                                ] ;
                cco:CCO_0002036 "http://wordnetweb.princeton.edu/perl/webwn?s=day" ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002059 "Unless otherwise specified, a Day is assumed to be relative to the rotational motion of the Earth and is approximately equal to a twenty-four hour temporal region for which the start and end times are specified by the Temporal Reference System used."@en ;
                cco:CCO_0002060 "A One-Dimensional Temporal Region that is equal to the approximate length of time required for an Astronomical Body to complete one rotation around its Rotational Axis as specified relative to a particular Temporal Reference System."@en ;
                rdfs:label "Day"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009007
cco:CCO_0009007 rdf:type owl:Class ;

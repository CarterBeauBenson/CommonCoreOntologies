###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009000
cco:CCO_0009000 rdf:type owl:Class ;
                rdfs:subClassOf obo:BFO_0000038 ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty cco:CCO_0009037 ;
                                  owl:someValuesFrom cco:CCO_0009008
                                ] ;
                cco:CCO_0002036 "http://wordnetweb.princeton.edu/perl/webwn?s=month" ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002059 "Unless otherwise specified, it is assumed that a Month is specified relative to the Rotational Motion of the Moon around the Earth. Earth Months are approximately equal to four consecutive Weeks or 30 consecutive Days."@en ;
                cco:CCO_0002060 "A One-Dimensional Temporal Region that is based on the approximate length of time required for a Natural Satellite to complete one cycle of its rotational motion around its Primary Body."@en ;
                rdfs:label "Month"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009001
cco:CCO_0009001 rdf:type owl:Class ;
                rdfs:subClassOf cco:CCO_0009010 ;
                cco:CCO_0002036 "https://www.defit.org/julian-date/" ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Calendar Day in the Julian Calendar."@en ;
                rdfs:comment "A Julian Day begins at noon Universal Time and is twenty-four Hours in duration."@en ;
                rdfs:label "Julian Day"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009002
cco:CCO_0009002 rdf:type owl:Class ;
                rdfs:subClassOf cco:CCO_0009011 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Calendar Year in the Julian Calendar."@en ;
                rdfs:comment "A Julian Year begins concurrently with January 1, ends concurrently with December 31, and has an average duration of exactly 365.25 Julian Days. Julian Years are typically indicated by prefixing a capital 'J' in front of the Year number, e.g. J2000.0 or J2018."@en ;
                rdfs:label "Julian Year"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009003
cco:CCO_0009003 rdf:type owl:Class ;
                rdfs:subClassOf cco:CCO_0009010 ;
                cco:CCO_0002037 "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/TimeOntology"^^xsd:anyURI ;
                cco:CCO_0002060 "A Calendar Day in the Gregorian Calendar."@en ;
                rdfs:comment "A Gregorian Day is twenty-four Hours in duration."@en ;
                rdfs:label "Gregorian Day"@en .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CCO_0009004
cco:CCO_0009004 rdf:type owl:Class ;
                rdfs:subClassOf obo:BFO_0000038 ,
